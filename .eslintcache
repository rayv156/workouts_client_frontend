revent page refresh\n        const token = await window.localStorage.getItem(\"token\")\n        await fetch(\"http://localhost:3000/logs/\" + gState.selectedLog.id, {\n          method: \"put\",\n          headers: {\n            \"Content-Type\":\"application/json\",\n            \"Authorization\": `bearer ${token}`\n          },\n          body: JSON.stringify(formData)\n        })\n        history.push(\"/\")\n      }\n\n      return (\n          <div>\n        <h1>Update Log</h1>\n        <form onSubmit={handleUpdate}>\n        <label>Duration: </label>\n        <input type=\"time\" className=\"without_ampm\" name=\"duration\" value={formData.duration} onChange={createChange}/>\n        <label>Workout Type: </label>\n          <input type=\"text\" name=\"workout\" value={formData.workout} onChange={createChange}/>\n          <label>Notes: </label>\n          <input type=\"text\" name=\"notes\" value={formData.notes} onChange={createChange}/>\n          <input type=\"hidden\" name=\"user_id\" value={formData.user_id}/>\n          <input type=\"submit\" value=\"Update Log\"/>\n        </form>\n        </div>\n      )\n}\n\nexport default Update","/Users/rayvelasquez/React/workouts_client/src/index.js",[],["62","63"],"/Users/rayvelasquez/React/workouts_client/src/components/Signup.js",["64"],"/Users/rayvelasquez/React/workouts_client/src/components/Navigation.js",["65","66"],"/Users/rayvelasquez/React/workouts_client/src/components/NavSignedIn.js",[],"/Users/rayvelasquez/React/workouts_client/src/components/Login.js",[],"/Users/rayvelasquez/React/workouts_client/src/components/Logs.js",["67"],"/Users/rayvelasquez/React/workouts_client/src/components/Landing.js",["68","69","70"],"/Users/rayvelasquez/React/workouts_client/src/reportWebVitals.js",[],"/Users/rayvelasquez/React/workouts_client/src/components/Workouts.js",["71","72","73"],{"ruleId":"74","severity":1,"message":"75","line":6,"column":20,"nodeType":"76","messageId":"77","endLine":6,"endColumn":29},{"ruleId